.filter_slider
    width 100%
    &__select
        background: #f0f0f0
        width: 100%
        height: 6px
        position: relative
        .ui-slider-range
            background: #f16d7f
            height: 100%
            position: absolute
        .ui-slider-handle
            height: 14px
            width: 14px
            top: -4px
            border-radius 50%
            display: block
            background: #f16d7f
            position: absolute
            margin-left: -7px
    &__ranges
        font-family: OpenSans
        color:  #6f6e6e
        font-size: 14px
        font-weight: 400
        margin-top: 20px
        justify-content: space-between
        display: flex